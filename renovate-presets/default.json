{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "description": "Default Renovate presets",
  "extends": [
    "config:base",
    ":disableDependencyDashboard",
    ":prHourlyLimitNone",
    "group:testNonMajor"
  ],
  "prConcurrentLimit": 8,
  "addLabels": [
    "dependencies"
  ],
  "internalChecksFilter": "strict",
  "rebaseWhen": "conflicted"
}
